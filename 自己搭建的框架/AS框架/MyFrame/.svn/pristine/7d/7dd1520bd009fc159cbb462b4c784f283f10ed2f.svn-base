<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/backgroud_shape"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/dialog_content"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:gravity="center"
        android:paddingTop="@dimen/px88"
        android:text="content"
        android:textSize="@dimen/size36"
        android:textColor="@color/typeface_five" />
    
    <TextView
        android:id="@+id/dialog_content_2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center"
        android:gravity="center"
        android:paddingTop="@dimen/px10"
        android:paddingBottom="@dimen/px88"
        android:textSize="@dimen/size24"
        android:textColor="@color/typeface_seven" />

    <include layout="@layout/view_line" />

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <com.alnton.myframecore.view.Button.ClickEffectButton
            android:id="@+id/dialog_cancel"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/px88"
            android:layout_weight="1"
            android:background="@null"
            android:text="@string/cancel"
            android:textSize="@dimen/size33"
            android:textColor="@color/typeface_two" />

        <View
            android:layout_width="1dp"
            android:layout_height="fill_parent"
            android:background="@color/divider" />

        <com.alnton.myframecore.view.Button.ClickEffectButton
            android:id="@+id/dialog_confim"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/px88"
            android:layout_weight="1"
            android:background="@null"
            android:text="@string/confirm"
            android:textSize="@dimen/size33"
            android:textColor="@color/typeface_six" />
    </LinearLayout>

</LinearLayout>