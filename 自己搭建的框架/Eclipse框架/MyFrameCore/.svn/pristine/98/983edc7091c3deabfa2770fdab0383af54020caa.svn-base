package com.alnton.myFrameCore.ftp;

/**
 * <FTP返回的状态码>
 * @author  王乾州
 */
public enum FTPStatus
{
    /** 连接失败 */
    Connect_Fail,
    
    /** 登陆失败 */
    Login_Fail,
    
    /*************FTP上传*************/
    
    /** 创建目录成功 */
    Create_Directory_Success,
    
    /** 创建目录失败 */
    Create_Directory_Fail,
    
    /** 上传新文件成功 */
    Upload_New_File_Success,
    
    /** 上传新文件失败 */
    Upload_New_File_Failed,
    
    /** 文件存在 */
    File_Exits,
    
    /** 远程文件大于本地文件，上传终止 */
    Remote_Bigger_Local,
    
    /** 断点上传成功 */
    Upload_From_Break_Success,
    
    /** 断点上传失败 */
    Upload_From_Break_Failed,
    
    /** 删除远程文件失败 */
    Delete_Remote_Faild,
    
    /** 删除远程文件成功 */
    Delete_Remote_Success,
    
    
    
    /*************FTP下载*************/
    /** 远程文件不存在 */
    Remote_File_Noexist,
    
    /** SD卡内存不足 */
    SD_MEMERY_NOT_ENOUGH,
    
    /** 本地文件大于远程文件，下载中止 */
    Local_Bigger_Remote,
    
    /** 断点续传成功 */
    Download_From_Break_Success,
    
    /** 断点续传失败 */
    Download_From_Break_Failed,
    
    /** 新下载成功 */
    Download_New_Success,
    
    /** 新下载失败 */
    Download_New_Failed;
}